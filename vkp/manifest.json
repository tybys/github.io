{
  "manifest_version": 2,
  "name": "Lost Time",
  "version": "1.0",
  
  "icons": {
    "128": "default128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ],
	  "permissions": [
		"tabs"
	  ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ],

      "browser_action": {
        "default_title": "LostTime",
        "default_icon": "/default.png",
        "default_popup": "popup.html"
    }
    
}