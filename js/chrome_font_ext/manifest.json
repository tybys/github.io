{
    "manifest_version": 2,

    "name": "property picker",
    "version": "1.0",

    "icons": {
        "16": "16x16.png",
        "32": "32x32.png",
        "48": "48x48.png"
    },

        "permissions": [
             "<all_urls>"
        ],

    "browser_action": {
        "default_title": " ",
        "default_icon": "48x48.png",
        "default_popup": "popup.html"
    }
}